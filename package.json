{
  "name": "ExpressVueStarter",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run cleanWebpack && npm run buildClientProd",
    "buildClientProd": "npm run cleanClient && webpack -p --config webpack.prod.config.js --progress",
    "buildClientDev": "npm run cleanClient && webpack",
    "cleanWebpack": "rimraf node_modules/webpack/node_modules/uglifyjs-webpack-plugin",
    "cleanClient": "rimraf public/*",
    "startDev": "npm run buildClientDev && node --inspect server.js",
    "startProd": "npm run buildClientProd && node server.js",
    "serverOnly": "node server.js",
    "uiOnly": "webpack -w"
  },
  "engines": {
    "node": ">=8.1.4"
  },
  "dependencies": {
    "base64url": "^2.0.0",
    "body-parser": "~1.15.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "crypto": "0.0.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "express-session": "^1.15.6",
    "morgan": "~1.7.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "autoprefixer": "^7.1.2",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.25.0",
    "css-loader": "^0.28.4",
    "es6-promise": "^4.1.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "uglify-es": "^3.3.5",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "vue": "^2.4.2",
    "vue-axios": "^2.0.2",
    "vue-loader": "^13.0.2",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1"
  }
}
